{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "IAR Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/GN_Project.elf",
            "cwd": "${workspaceFolder}",
            "miDebuggerPath": "C:/Program Files/IAR Systems/Embedded Workbench 9.2/common/bin/CSpyServer2.exe",
            "miDebuggerArgs": "--backend --endian=little --cpu=Cortex-M0+ --fpu=None --semihosting --config=C:/GN_Project/Code/sys/stm32g071xx_flash.icf --drv=ST-LINK --port=SWD --device=STM32G071CB",
            "setupCommands": [
                {
                    "text": "target remote :2331"
                },
                {
                    "text": "monitor reset halt"
                },
                {
                    "text": "monitor reset init"
                }
            ],
            "preLaunchTask": "Build GN Project",
            "externalConsole": false,
            "MIMode": "gdb"
        },
